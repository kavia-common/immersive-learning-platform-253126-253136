# React LMS - Supabase and App Environment
# Copy this file to .env and fill in values. Do NOT commit .env.

# App identity
REACT_APP_APP_NAME=Ocean LMS
# Public site URL (used for auth email redirects)
REACT_APP_SITE_URL=http://localhost:3000

# Supabase project configuration (public, safe for frontend)
REACT_APP_SUPABASE_URL=https://YOUR-PROJECT-REF.supabase.co
# Prefer ANON key name; older configs may use REACT_APP_SUPABASE_KEY
REACT_APP_SUPABASE_ANON_KEY=ey...your_anon_key
# Backward-compat alias (optional)
# REACT_APP_SUPABASE_KEY=ey...your_anon_key

# API / Routing (optional)
REACT_APP_API_BASE=
REACT_APP_BACKEND_URL=
REACT_APP_FRONTEND_URL=http://localhost:3000
REACT_APP_WS_URL=

# Runtime / Build flags
REACT_APP_NODE_ENV=development
REACT_APP_NEXT_TELEMETRY_DISABLED=true
REACT_APP_ENABLE_SOURCE_MAPS=true
REACT_APP_PORT=3000
REACT_APP_TRUST_PROXY=false
REACT_APP_LOG_LEVEL=info
REACT_APP_HEALTHCHECK_PATH=/healthz
REACT_APP_FEATURE_FLAGS=
REACT_APP_EXPERIMENTS_ENABLED=false
